{
    "dataform-version": "1.0.0",
    "size": {
        "h": 300,
        "w": 500
    },
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [
        {
            "type": "textbox",
            "field": "mapModelNodeId",
            "title": "Map model node:",
            "tooltip": "Node of the map model node",
            "required": true
        },
        {
            "type": "selectbox",
            "field": "swipeLayerWidgetOpts.type",
            "title": "Type:",
            "tooltip": "Choose style of swipe widget",
            "values": [
                "vertical",
                "horizontal",
                "scope"
            ]
        }
    ]
}